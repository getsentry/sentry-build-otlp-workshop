{
  "name": "otel-ecommerce",
  "version": "1.0.0",
  "private": true,
  "description": "OpenTelemetry E-commerce Demo - Sentry Frontend + OTEL Backend",
  "scripts": {
    "api": "cd api && npm start",
    "api:dev": "cd api && npm run dev",
    "cache:clear": "cd api && npm run cache:clear",
    "collector:all": "cd api && npm run collector:all",
    "collector:cleanup": "cd api && npm run collector:cleanup",
    "collector:gateway": "cd api && npm run collector:gateway",
    "collector:health": "cd api && npm run collector:health",
    "collector:help": "cd api && npm run collector:help",
    "collector:logs": "cd api && npm run collector:logs",
    "collector:orders": "cd api && npm run collector:orders",
    "collector:products": "cd api && npm run collector:products",
    "collector:start": "cd api && npm run collector:start",
    "collector:stop": "cd api && npm run collector:stop",
    "db:init": "cd api && npx neondb -y",
    "db:setup": "cd api && npm run db:setup",
    "demo:collector": "echo '\nðŸš€ COLLECTOR MODE: Starting all microservices...\n' && cd api && npm run collector:all",
    "demo:direct": "echo '\nðŸŽ¯ DIRECT MODE: Sending telemetry directly to Sentry\n' && cd api && npm run direct",
    "dev:frontend": "cd frontend && npm run dev",
    "direct": "cd api && npm run direct",
    "frontend": "cd frontend && npm run dev",
    "frontend:build": "cd frontend && npm run build",
    "install:all": "npm install && cd api && npm install && cd ../frontend && npm install",
    "test:api": "cd api && npm run test"
  },
  "keywords": [
    "opentelemetry",
    "otel",
    "sentry",
    "distributed-tracing",
    "react",
    "express"
  ],
